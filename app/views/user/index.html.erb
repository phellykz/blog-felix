<h1>Listing users</h1>


<table class="table table-striped">
  <thead>
    <tr>
        
      <th>Email</th>
    
      <th>Email</th>

    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>        
        <td><%= user.email %></td>
        <td><%= user.role %></td>   
      </tr>
    <% end %>
  </tbody>
</table>
